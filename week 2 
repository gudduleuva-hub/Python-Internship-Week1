number_guess.py
import random

print("ğŸ¯ Number Guessing Game")

secret = random.randint(1, 10)

while True:
    guess = int(input("Guess a number between 1 to 10: "))

    if guess == secret:
        print("Correct! ğŸ‰ You guessed the number!")
        break
    elif guess < secret:
        print("Too Low, try again!")
    else:
        print("Too High, try again!")
